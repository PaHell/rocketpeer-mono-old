{
  "$schema": "..\\..\\..\\node_modules/@tauri-apps/cli\\schema.json",
  "build": {
    "beforeBuildCommand": "npm run build -w rocketpeer-sveltekit",
    "beforeDevCommand": "npm run dev -w rocketpeer-sveltekit",
    "devPath": "http://localhost:3000",
    "distDir": "../sveltekit/static"
  },
  "package": {
    "productName": "rocketpeer-tauri",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "SocialNetworking",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.redesiigner.rocketpeer",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "allowDowngrades": false,
        "tsp": true
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "title": "rocketpeer",
        "label": "main",
        "fullscreen": false,
        "resizable": true,
        "width": 1366,
        "height": 864,
        "minWidth": 800,
        "minHeight": 600,
        "center": true,
        "focus": true,
        "decorations": true
      }
    ],
    "systemTray": {
      "iconPath": "icons/32x32.png"
    }
  }
}
